{
  "globals": {
    "ts-jest": {
      "tsConfig": "tsconfig.json"
    }
  },
  "moduleFileExtensions": ["ts", "js"],
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "testMatch": ["**/test/**/*.test.ts"],
  "testEnvironment": "node",
  "coveragePathIgnorePatterns": ["/node_modules/", "build", "src/migrations"]
}
