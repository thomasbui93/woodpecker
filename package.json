{
   "name": "woodpecker",
   "version": "0.0.1",
   "description": "Woodpecker - Woodpecker CMS system.",
   "main": "src/index.ts",
   "scripts": {
      "prestart": "npm install && npm run build",
      "start": "ts-node src/index.ts",
      "dev": "nodemon src/index.ts",
      "build": "tsc",
      "lint": "eslint . --ext .ts",
      "lint:fix": "eslint . --ext .ts --fix",
      "test": "jest --coverage --verbose --passWithNoTests --config=jest.config.json",
      "migration:create": "typeorm migration:create",
      "migration:run": "npm run build && typeorm migration:run"
   },
   "author": "Khoa Bui <buidangkhoa2602@gmail.com>",
   "license": "MIT",
   "devDependencies": {
      "@types/body-parser": "^1.19.0",
      "@types/compression": "^1.7.0",
      "@types/cors": "^2.8.8",
      "@types/express": "^4.17.8",
      "@types/jest": "^26.0.14",
      "@types/node": "^8.0.29",
      "@types/pino": "^6.3.3",
      "@types/supertest": "^2.0.10",
      "@typescript-eslint/eslint-plugin": "^4.5.0",
      "@typescript-eslint/parser": "^4.5.0",
      "eslint": "^7.11.0",
      "eslint-config-airbnb-base": "^14.2.0",
      "eslint-plugin-import": "^2.22.1",
      "eslint-plugin-jsdoc": "^30.6.2",
      "eslint-plugin-no-null": "^1.0.2",
      "jest": "^26.5.3",
      "nodemon": "^2.0.5",
      "sqlite3": "^5.0.0",
      "supertest": "^5.0.0",
      "ts-jest": "^26.4.1",
      "ts-node": "^9.0.0",
      "typescript": "^4.0.3"
   },
   "dependencies": {
      "body-parser": "^1.19.0",
      "compression": "^1.7.4",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "mysql": "^2.14.1",
      "pino": "^6.7.0",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.28"
   }
}
